{
  "name": "sfra-startup-kit",
  "version": "1.0.0",
  "repository": "git@github.com:OSFGlobal/SFRA-Startup-Kit.git",
  "author": "Daniel Anechitoaie <daniel.anechitoaie@osf.digital>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "sq:js": "eslint -f json \"cartridges/**/*.js\"",
    "sq:css": "stylelint -f json \"cartridges/**/*.scss\"",
    "test": "mocha test/unit/**/*.js && lerna run test",
    "test:unit": "mocha test/unit/**/*.js",
    "cover": "nyc --report-dir coverage/summary mocha test/unit/**/*.js | tee coverage/report-$(date \"+%Y-%m-%d_%H-%M-%S\")",
    "cover:local": "nyc --reporter=lcov --reporter=text-summary --report-dir coverage/summary mocha test/unit/**/*.js",
    "cover:html": "nyc --reporter=lcov --report-dir coverage/summary mocha test/unit/**/*.js",
    "test:class": "sgmf-scripts --test",
    "dev": "cross-env NODE_ENV=development webpack --mode=development --devtool=inline-source-map --watch",
    "prd": "cross-env NODE_ENV=production webpack --mode=production",
    "analyze": "cross-env NODE_ENV=production webpack --mode=production --env analyze",
    "lerna": "lerna bootstrap",
    "lerna:build": "lerna run build",
    "lerna:test": "lerna run test",
    "generate:metadata": "node bin/generators/metadata.js",
    "lint:js": "eslint \"cartridges/**/*.js\"",
    "lint:scss": "stylelint --allow-empty-input \"cartridges/**/*.scss\"",
    "lint:isml": "isml-linter --build",
    "fix:js": "eslint --fix \"cartridges/**/*.js\"",
    "fix:scss": "stylelint --allow-empty-input --fix \"cartridges/**/*.scss\"",
    "fix:isml": "isml-linter --autofix"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "7.26.9",
    "@babel/runtime": "7.26.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.1",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-sitegenesis": "^1.0.0",
    "expose-loader": "^4.0.0",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "isml-linter": "^5.43.0",
    "jsdom": "^26.1.0",
    "lerna": "^7.0.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^1.4.1",
    "minimatch": "^3.0.4",
    "nyc": "^15.1.0",
    "prettier": "2.7.1",
    "proxyquire": "^2.1.3",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sgmf-scripts": "3.0.0",
    "sinon": "^14.0.1",
    "sinon-chai": "^3.7.0",
    "stylelint": "^14.16.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "webpack": "^5.32.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.6.0",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "autoprefixer": "^10.2.5",
    "bootstrap": "^4.4.1",
    "browser-sync": "2.27.12",
    "cleave.js": "^1.6.0",
    "cross-env": "^7.0.3",
    "dompurify": "^3.2.6",
    "gsap": "^3.13.0",
    "inputmask": "^5.0.3",
    "jquery": "^3.5.0",
    "lenis": "^1.3.11",
    "lodash.debounce": "^4.0.8",
    "lodash.intersection": "^4.4.0",
    "photoswipe": "^4.1.3",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.29",
    "postcss-loader": "7.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "simplebar": "6.3.0",
    "sticky-kit": "^1.1.3",
    "swiper": "^12.0.2"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 10 versions",
    "> 1%"
  ],
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "**/*": "true && `# revert to prettier command post code freeze: prettier --write --ignore-unknown`"
  },
  "resolutions": {
    "**/ansi-regex": "^5.0.1",
    "**/qs": "^6.11.0"
  },
  "volta": {
    "node": "16.20.2",
    "yarn": "1.22.22"
  }
}
